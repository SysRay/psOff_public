{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "(Windows) Launch",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${workspaceRoot}/_build/_Install/psoff.exe",
      "args": [
        "--d",
        // "${config:psoff.add-arg}",
        // "--file=${config:psoff.game-path}/eboot.bin",
        "--pipe=\\\\.\\pipe\\psoff-test",
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceRoot}/_build/_Install",
      "environment": [],
      "console": "integratedTerminal",
      "symbolSearchPath": "${workspaceRoot}/_build/_Install/debug"
    },
    {
      "name": "(Windows) Attach",
      "type": "cppvsdbg",
      "request": "attach",
      "processId": "${command:pickProcess}"
    },
    {
      "name": "(Windows) Launch piped",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${workspaceRoot}/_build/_Install/mainipc.exe",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceRoot}/_build/_Install",
      "environment": [],
      "console": "integratedTerminal",
    }
  ]
}
